<script>
    function changeMenu(elementId) {
        // 선택된 element
        let selectedElement = document.getElementById(elementId);

        document.getElementById("central").classList.remove("active");
        document.getElementById("education").classList.remove("active");
        document.getElementById("aram").classList.remove("active");

        document.getElementById(elementId).classList.add("active");


        let restaurant = ["central", "education", "aram"];
        restaurant.forEach(d1 => {
            Array.from(document.getElementsByClassName(d1 + "_menu")).forEach(d2 => {
                d2.style.display = "none";
            });
        });

        Array.from(document.getElementsByClassName(elementId + "_menu")).forEach(d2 => {
            d2.style.display = "table-row";
        });

    }

</script>

