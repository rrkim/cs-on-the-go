                    <div class="food-data-area">
                        <div class="d-flex justify-content-center position-relative">
                            <p class="fs-3"><span class="fw-bold">식단표</span></p>
                            <a href="/foodMenu" class="position-right-1">더보기</a>
                        </div>
                        <div class="card index-info-card mt-3">
                            <div class="card-body">
                                <div>
                                    <h6 id="central" class="active color-gray cursor-pointer d-inline" onclick="changeMenu('central')">중앙식당</h6>
                                    <h6 id="education" class="color-gray cursor-pointer d-inline" onclick="changeMenu('education')">교육문화센터 식당</h6>
                                    <h6 id="aram" class="color-gray cursor-pointer d-inline" onclick="changeMenu('aram')">아람관</h6>
                                </div>
                                <table class="table01 mt-3 ">
                                    <tr>
                                        <th>조식</th>
                                        <th>중식</th>
                                        <th>석식</th>
                                    </tr>
                                    <tr id="meal">
                                        <td class="text-center">[천원의 아침밥]<br>새우완탕<br>[조식]<br>라면/김밥</td>
                                        <td class="text-center">
                                            갈비김치찜<br>생협치즈돈까스<br>(세트메뉴)<br>소불고기보쌈정식
                                        </td>
                                        <td class="text-center">불닭치즈돌솥비빔밥</td>
                                    </tr>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%- include(getViewPath("/core/views/common/footer.ejs")) %>
</body>
<script>
    $(function () {
        $(".menu-trigger").on('click', function () {
            $(this).toggleClass('active');
            $('.header-area .menu li').slideToggle(200);
        });

        $('.main-banner').slick({
            slide: 'div',
            infinite: true,
            slidesToScroll: 1,
            speed: 100,
            arrows: true,
            dots: true,
            autoplay: true,
            autoplaySpeed: 10000,
            pauseOnHover: true,
            vertical: false,
            dotsClass: "slick-dots",
            draggable: true
        });

        $(".university-progress .university-progress-bar .runner").each(function (i, iv) {
            let percent = $(iv).data("completePercent");
            $(iv)[0].style.setProperty("--cs-on-the-go-runner-percent", percent);
        });
    });

    const newMealData = {
        central: [
            "[천원의 아침밥]<br>새우완탕<br>[조식]<br>라면/김밥",
            "갈비김치찜<br>생협치즈돈까스<br>(세트메뉴)<br>소불고기보쌈정식",
            "불닭치즈돌솥비빔밥"
        ],
        education: [
            "정보 없음",
            "[양식]<br>고구마치즈돈까스<br>[비빔밥]<br>불닭생채비빔밥",
            "정보 없음"
        ],
        aram: [
            "[한식]<br>쌀밥/누룽지<br>감자맑은국<br>두부스틱강정...",
            "[한식]<br>쌀밥<br>두부김치찌개<br>떡갈비조림...",
            "[한식]<br>쌀밥<br>닭개장<br>메밀전병..."
        ],
    };

    function changeMenu(elementId) {
        document.getElementById("central").classList.remove("active");
        document.getElementById("education").classList.remove("active");
        document.getElementById("aram").classList.remove("active");

        document.getElementById(elementId).classList.add("active");

        const mealTableData = document.getElementById("meal").querySelectorAll("td");
        mealTableData.forEach((iv, i) => {
            iv.innerHTML = newMealData[elementId][i];
        });
    }

</script>

</html>